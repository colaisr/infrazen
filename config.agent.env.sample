# Agent Service Configuration Sample
# Copy to config.agent.env and fill in actual values

# Service configuration
AGENT_PORT=8001
AGENT_ENV=dev
LOG_LEVEL=INFO

# InfraZen integration
AGENT_INTERNAL_API_BASE=http://127.0.0.1:5001/internal
AGENT_SERVICE_JWT_SECRET=change-this-secret-in-production

# LLM Provider configuration
LLM_PROVIDER=openrouter
LLM_MODEL_TEXT=openai/gpt-4o-mini
LLM_MODEL_VISION=openai/gpt-4o

# Provider API keys (fill at least one)
OPENROUTER_API_KEY=
OPENAI_API_KEY=
ANTHROPIC_API_KEY=

# Redis (session store)
REDIS_URL=redis://127.0.0.1:6379/0

# Vector store
VECTOR_STORE=chroma

